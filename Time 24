import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner obj = new Scanner(System.in);
        int a =obj.nextInt();
        int b =obj.nextInt();
        int c =obj.nextInt();
        b+=c/60;
        c=c%60;
        a+=b/60;
        b=b%60;
        System.out.printf("Total Number of hours is %d",a);
        System.out.printf("\nTotal Number of minutes is %d",b);
        System.out.printf("\nTotal Number of seconds is %d",c);
        
    }
}
